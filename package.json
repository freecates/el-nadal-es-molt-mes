    "react-burger-menu": "^2.6.12",
    "react-dom": "16.11.0",
    "react-intl": "^3.2.3",
    "react-parallax": "^2.2.3",
    "react-responsive": "^8.0.1",
    "react-responsive-carousel": "^3.1.51",
